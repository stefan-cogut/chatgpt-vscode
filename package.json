{
  "name": "fintechos-ai",
  "displayName": "FintechOS AI",
  "description": "Automatically write new code, ask questions, find bugs, and more with FintechOS AI",
  "version": "0.1.0",
  "publisher": "Fintech OS",
  "icon": "resources/icon.png",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/stefan-cogut/chatgpt-vscode"
  },
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [],
  "activationEvents": [
    "onView:chatgpt-ai.chatView",
    "onCommand:chatgpt-ai.ask",
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "chatgpt-ai.ask",
        "title": "Ask FintechOS AI"
      },
      {
        "command": "chatgpt-ai.resetConversation",
        "title": "Reset conversation"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "chatgpt-ai.ask",
          "when": "editorTextFocus",
          "group": "fintechos-menu-group@1"
        }
      ],
      "commandPalette": [
        {
          "command": "chatgpt-ai.ask"
        },
        {
          "command": "chatgpt-ai.resetConversation"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "chatgpt",
          "title": "FintechOS AI",
          "icon": "resources/icon.png"
        }
      ]
    },
    "views": {
      "chatgpt": [
        {
          "type": "webview",
          "id": "chatgpt-ai.chatView",
          "name": "FintechOS AI"
        }
      ]
    },
    "configuration": {
      "title": "FintechOS AI",
      "type": "object",
      "properties": {
        "chatgpt-ai.apiKey": {
          "markdownDescription": "The FintechOS Subscription Key.",
          "type": "string",
          "order": 10
        },
        "chatgpt-ai.apiBaseUrl": {
          "type": "string",
          "default": "https://api.openai.com/v1",
          "description": "The URL for the FintechOS AI Service API",
          "order": 11
        },
        "chatgpt-ai.indentOnInserting": {
          "type": "boolean",
          "order": 18,
          "markdownDescription": "Indent the code snippet on inserting it to the source code.",
          "default": true
        },
        "chatgpt-ai.keepConversation": {
          "type": "boolean",
          "default": true,
          "description": "Keep the conversation going by using the same conversation ID for all requests (allows follow-up questions)",
          "order": 80
        },
        "chatgpt-ai.timeoutLength": {
          "type": "number",
          "default": 120,
          "description": "How long should the request wait for a response before timing out (in seconds)",
          "order": 90
        },
        "chatgpt-ai.selectedInsideCodeblock": {
          "type": "boolean",
          "default": true,
          "description": "Append selected code as a codeblock (```...code...```) instead of plain text",
          "order": 100
        },
        "chatgpt-ai.codeblockWithLanguageId": {
          "type": "boolean",
          "default": true,
          "description": "Append language id of the selected code to the codeblock (```language...code...```)",
          "order": 101
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/jquery": "^3.5.16",
    "@types/jqueryui": "^1.12.16",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "chatgpt": "^5.2.5",
    "cheerio": "^1.0.0-rc.12",
    "csv": "^6.2.8",
    "domutils": "^3.1.0",
    "fetch": "^1.1.0",
    "gpt3-tokenizer": "^1.1.5",
    "highlight.js": "^11.7.0",
    "jquery": "^3.6.4",
    "jquery-ui": "^1.13.2",
    "marked": "^4.3.0",
    "node-fetch": "^3.3.1"
  }
}
